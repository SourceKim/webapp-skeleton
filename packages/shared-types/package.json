{
  "name": "@skeleton/shared-types",
  "version": "1.0.0",
  "description": "Skeleton 项目共享 TypeScript 类型定义",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "types",
    "typescript",
    "shared",
    "skeleton"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "zod": "^4.1.13"
  }
}
